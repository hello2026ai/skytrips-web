{
  "name": "skytripsv1",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/skytripsv1",
  "projectType": "application",
  "tags": [],
  "// targets": "to see all targets run: nx show project skytripsv1 --web",
  "targets": {
    "build": {
      "executor": "@nx/next:build",
      "outputs": ["{options.outputPath}", "{options.outputPath}/.next"],
      "options": {
        "outputPath": "dist/apps/skytripsv1",
        "root": "apps/skytripsv1"
      }
    },
    "serve": {
      "executor": "@nx/next:server",
      "options": {
        "buildTarget": "skytripsv1:build",
        "dev": false,
        "port": 3500
      }
    },
    "dev": {
      "executor": "@nx/next:server",
      "options": {
        "buildTarget": "skytripsv1:build",
        "dev": true,
        "port": 3500
      }
    }
  }
}
